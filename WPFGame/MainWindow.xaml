<Window x:Class="WPFGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFGame"
        mc:Ignorable="d"
        Title="MainWindow" Width="800" Height="600" ResizeMode="NoResize"
        KeyDown="Window_KeyDown" KeyUp="Window_KeyUp">
    <Grid>
        <Canvas x:Name="GameCanvas" Width="800" Height="600">
            <Image x:Name="GameImage" Width="800" Height="600">
                
            </Image>
        </Canvas>
    </Grid>
</Window>
